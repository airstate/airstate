{
    "name": "@airstate/server",
    "version": "0.1.7",
    "description": "The main server for AirState",
    "main": "./dist/index.mjs",
    "type": "module",
    "private": true,
    "scripts": {
        "start": "node .",
        "start:watch": "linkemon --inspect=0.0.0.0:${AIRSTATE_DEBUG_PORT:-15001} .",
        "build": "tsc",
        "build:no-check": "tsc || exit 0",
        "build:watch": "tsc --watch --preserveWatchOutput",
        "dev": "concurrently pnpm:build:watch pnpm:start:watch",
        "postinstall": "./postinstall.sh"
    },
    "dependencies": {
        "@t3-oss/env-core": "0.12.0",
        "@trpc/client": "11.1.2",
        "@trpc/server": "11.1.2",
        "async-iterable-queue": "^1.0.16",
        "bufferutil": "4.0.9",
        "cookie": "^1.0.2",
        "dbly-linked-list": "^0.3.5",
        "es-toolkit": "^1.37.2",
        "express": "4.21.2",
        "iovalkey": "^0.3.1",
        "ip-location-api": "3.0.3",
        "jsonwebtoken": "9.0.2",
        "mobx": "^6.13.7",
        "nanoid": "5.1.5",
        "nats": "2.29.0",
        "private-ip": "^3.0.2",
        "scope-utilities": "^2.2.1",
        "ua-parser-js": "^2.0.3",
        "winston": "3.17.0",
        "ws": "8.18.0",
        "yjs": "13.6.24",
        "zod": "3.24.1"
    },
    "devDependencies": {
        "@types/express": "4.17.21",
        "@types/jsonwebtoken": "9.0.9",
        "@types/node": "22.12.0",
        "@types/ws": "8.5.12",
        "concurrently": "9.1.2",
        "linkemon": "0.2.0",
        "nodemon": "3.1.9",
        "typescript": "5.8.3"
    },
    "engines": {
        "node": "22.12.0"
    },
    "packageManager": "pnpm@10.12.1",
    "files": [
        "dist"
    ]
}
